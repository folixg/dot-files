{
    "files.trimTrailingWhitespace": true,
    "telemetry.telemetryLevel": "off",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.fontFamily": "'Sauce Code Pro','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.accessibilitySupport": "off",
    "editor.renderControlCharacters": true,
    "editor.showFoldingControls": "always",
    "editor.cursorStyle": "line",
    "editor.lineNumbers": "on",
    "editor.renderWhitespace": "boundary",
    "editor.trimAutoWhitespace": true,
    "editor.formatOnSave": true,
    "workbench.enableExperiments": false,
    "workbench.startupEditor": "none",
    "workbench.tips.enabled": false,
    "workbench.editor.labelFormat": "medium",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "window.menuBarVisibility": "compact",
    "window.customMenuBarAltFocus": false,
    "window.enableMenuBarMnemonics": false,
    "search.smartCase": true,
    "debug.inlineValues": true,
    "extensions.ignoreRecommendations": true,
    "python.venvPath": "~/.virtualenvs",
    "python.formatting.provider": "black",
    "python.languageServer": "Pylance",
    "vim.foldfix": true,
    "vim.hlsearch": true,
    "vim.leader": " ",
    "vim.handleKeys": {
        "<C-k>": false
    },
    "vim.useSystemClipboard": true,
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.duration": 400,
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "C_Cpp.clang_format_fallbackStyle": "llvm",
    "workbench.colorCustomizations": {
        "editor.lineHighlightBorder": "#243443",
        "editor.lineHighlightBackground": "#243443",
        "editorRuler.foreground": "#243443"
    },
    "editor.rulers": [
        80
    ],
    "zenMode.hideLineNumbers": false,
    "zenMode.hideStatusBar": false,
    "files.eol": "\n",
    "[markdown]": {
        "editor.tabSize": 2
    },
    "git.untrackedChanges": "separate",
    "terminal.integrated.defaultProfile.linux": "fish",
    "terminal.integrated.defaultProfile.osx": "fish",
    "terminal.integrated.fontFamily": "\"SauceCodePro NF\"",
    "problems.showCurrentInStatus": true,
    "workbench.preferredDarkColorTheme": "Dracula",
    "workbench.colorTheme": "Dracula",
}